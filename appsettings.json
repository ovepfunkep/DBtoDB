{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SqlServer": "Server=your_server;Database=your_db;User Id=your_user;Password=your_password;TrustServerCertificate=True",
    "PostgreSQL": "Host=your_host;Database=your_db;Username=your_user;Password=your_password"
  },
  "DatabaseSettings": {
    "CacheTimeoutMinutes": 5,
    "CommandTimeoutSeconds": 30,
    "MaxPoolSize": 100
  }
} 